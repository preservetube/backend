<% layout('./layout') %>

<div class="grid">
  <% it.data.forEach(function(r){ %>
    <a class="title" href="/transparency/<%= r.target %>"><%= r.title %></a>
  <% }) %>
</div>

<style> 
  .grid { 
    margin-top: 10px;
    margin-bottom: 10px;
    text-align: center;
  }

  .title {
    font-size: larger;
    font-weight: 600;
    display: inline-block;
  }
</style>